{
  "$schema": "https://schema.tauri.app/config/2",
  "identifier": "default",
  "description": "默认权限配置",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:allow-save",
    "dialog:allow-open",
    "dialog:default",
    {
      "identifier": "shell:allow-open",
      "allow": [
        { "path": "**" }
      ]
    }
  ]
}
